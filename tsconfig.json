{
  "version": "1.8.10",
  "compilerOptions": {
    "strict": true,
    "target": "es5",
    "module": "commonjs",
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "downlevelIteration": true,
    "declaration": true,
    "noEmit": false,
    "rootDir": ".",
    "outDir": "./lib",
    "lib": ["dom", "es2015", "scripthost"],
    "useDefineForClassFields": true,
  },
  "include": ["src"],
  "exclude": ["/node_modules"]
}
